@article{Indefrey2004,
Author = {Indefrey, P and Levelt, WJM},
Title = {{The spatial and temporal signatures of word production components}},
Journal = {{Cognition}},
Year = {{2004}},
Volume = {{92}},
Number = {{1-2}},
Pages = {{101--144}},
Month = {{MAY-JUN}},
Abstract = {{This paper presents the results of a comprehensive meta-analysis of the
   relevant imaging literature on word production (82 experiments). In
   addition to the spatial overlap of activated regions, we also analyzed
   the available data on the time course of activations. The analysis
   specified regions and time windows of activation for the core processes
   of word production: lexical selection, phonological code retrieval,
   syllabification, and phonetic/articulatory preparation. A comparison of
   the word production results with studies on auditory word/non-word
   perception and reading showed that the time course of activations in
   word production is, on the whole, compatible with the temporal
   constraints that perception processes impose on the production
   processes they affect in picture/word interference paradigms. (C) 2004
   Elsevier B.V. All rights reserved.}},
